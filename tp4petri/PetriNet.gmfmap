<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="PetriNet.ecore#//PetriNet/nodes"/>
    <ownedChild>
      <domainMetaElement
          href="PetriNet.ecore#//Transition"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="PetriNet.gmfgraph#TransitionName"/>
        <features
            href="PetriNet.ecore#//Node/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="PetriNet.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="PetriNet.gmfgraph#Transition"/>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="PetriNet.ecore#//PetriNet/nodes"/>
    <ownedChild>
      <domainMetaElement
          href="PetriNet.ecore#//Place"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="PetriNet.gmfgraph#PlaceName"/>
        <features
            href="PetriNet.ecore#//Node/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="PetriNet.gmftool#//@palette/@tools.0/@tools.1"/>
      <diagramNode
          href="PetriNet.gmfgraph#Place"/>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="PetriNet.ecore#//Arc"/>
    <containmentFeature
        href="PetriNet.ecore#//PetriNet/arcs"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="PetriNet.gmftool#//@palette/@tools.0/@tools.2"/>
    <diagramLink
        href="PetriNet.gmfgraph#Arc"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="PetriNet.ecore#//Arc/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="PetriNet.ecore#//Arc/target"/>
  </links>
  <diagram>
    <diagramCanvas
        href="PetriNet.gmfgraph#petriNet"/>
    <domainModel
        href="PetriNet.ecore#/"/>
    <domainMetaElement
        href="PetriNet.ecore#//PetriNet"/>
    <palette
        href="PetriNet.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
